        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyBnmN-fxzlNLKoQQS9c0S5hdzfYo2Ac_Cw" />

        <activity
            android:name="com.vn.pro.nguhanh.activity.HomeActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name="com.vn.pro.nguhanh.activity.LauncherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.vn.pro.nguhanh.activity.InfoActivity"
            android:screenOrientation="portrait"></activity>
